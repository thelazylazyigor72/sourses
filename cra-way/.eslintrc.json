{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": [
		"react-app",
		"react-app/jest",
		"airbnb",
		"plugin:prettier/recommended"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"max-len": "off",
		"react/function-component-definition": "off",
		"spaced-comment": "off",
		"react/prop-types": "off",
		"react/destructuring-assignment": "off",
		"react/no-array-index-key": "warn",
		"no-unused-vars": "warn"
	}
}
